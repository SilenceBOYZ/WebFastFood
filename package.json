{
  "name": "cafemisa",
  "version": "1.0.0",
  "description": "WebCafe",
  "main": "./src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:sass": "node-sass ./src/sass/main.scss ../public/css/style.css -w",
    "devserver": "live-server",
    "start": "nodemon --inspect ./src/server.js",
    "watch": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass ./src/sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SilenceBOYZ/ProcheckCoffeeMisa.git"
  },
  "keywords": [
    "Cafe",
    "javaScript"
  ],
  "author": "ThanhTri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SilenceBOYZ/ProcheckCoffeeMisa/issues"
  },
  "homepage": "https://github.com/SilenceBOYZ/ProcheckCoffeeMisa#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "autoprefix": "^1.0.1",
    "concat": "^1.0.3",
    "mysql2": "^3.6.5",
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss-cli": "^10.1.0",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2"
  }
}
